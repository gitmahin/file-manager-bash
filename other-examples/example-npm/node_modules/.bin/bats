#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/lxxxv/linux_files/workspace/shell-scripting/projects/filemanager/other-examples/example-npm/node_modules/.pnpm/bats@1.12.0/node_modules/bats/bin/node_modules:/media/lxxxv/linux_files/workspace/shell-scripting/projects/filemanager/other-examples/example-npm/node_modules/.pnpm/bats@1.12.0/node_modules/bats/node_modules:/media/lxxxv/linux_files/workspace/shell-scripting/projects/filemanager/other-examples/example-npm/node_modules/.pnpm/bats@1.12.0/node_modules:/media/lxxxv/linux_files/workspace/shell-scripting/projects/filemanager/other-examples/example-npm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/lxxxv/linux_files/workspace/shell-scripting/projects/filemanager/other-examples/example-npm/node_modules/.pnpm/bats@1.12.0/node_modules/bats/bin/node_modules:/media/lxxxv/linux_files/workspace/shell-scripting/projects/filemanager/other-examples/example-npm/node_modules/.pnpm/bats@1.12.0/node_modules/bats/node_modules:/media/lxxxv/linux_files/workspace/shell-scripting/projects/filemanager/other-examples/example-npm/node_modules/.pnpm/bats@1.12.0/node_modules:/media/lxxxv/linux_files/workspace/shell-scripting/projects/filemanager/other-examples/example-npm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../bats/bin/bats" "$@"
else
  exec bash  "$basedir/../bats/bin/bats" "$@"
fi
